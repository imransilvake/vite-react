{
	"name": "robot-gui",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"start": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"pretty:check": "prettier --check './**/*.{js,jsx,ts,tsx,json}'",
		"pretty:apply": "prettier --write './**/*.{js,jsx,ts,tsx,json}'",
		"lint:es": "npx eslint './**/*.{js,jsx,ts,tsx}' --quiet",
		"lint:es:fix": "npx eslint './**/*.{js,jsx,ts,tsx}' --fix",
		"scan:all": "yarn pretty:apply && yarn lint:es && yarn test:ci"
	},
	"dependencies": {
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"sass": "^1.59.3"
	},
	"devDependencies": {
		"@types/node": "^18.15.5",
		"@types/react": "^18.0.28",
		"@types/react-dom": "^18.0.11",
		"@typescript-eslint/eslint-plugin": "^5.56.0",
		"@typescript-eslint/parser": "^5.56.0",
		"@vitejs/plugin-react": "^3.1.0",
		"eslint": "^8.36.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-simple-import-sort": "^10.0.0",
		"prettier": "^2.8.6",
		"typescript": "^4.9.3",
		"vite": "^4.2.0"
	}
}
